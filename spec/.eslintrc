{
  "globals": {
    "DEBUG": false,
    "define": false,
    "describe": false,
    "before": false,
    "it": false,
    "after": false,
    "afterEach": false,
    "beforeEach": false
  },

  "ecmaFeatures": {
    "modules" : true
  },

  "env": {
    "es6": true
  },

  "rules": {
    "no-undef": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-underscore-dangle" : 0
  }
}